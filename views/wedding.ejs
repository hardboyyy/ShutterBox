<%- include('partials/header') -%>


<section id="banner">
    <div class="container">
      <div class="banner-caption">
        <div class="row justify-content-end align-items-center">
          <!-- <div class="col-sm-6"></div> -->
          <div class="col-sm-6 col-md-4 mt-5">
            <h2 class="text-uppercase">Wedding Photography</h2>
            <p>Are you looking for best wedding photographers in Mumbai, india?
                Look our creative wedding photography album here.</p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <br /> 
  <br /> 
  <br /> 

  <section id="wedding">
    <div class="container">
        <div class="row" id="album-gallery">
            <% wedding.map((wedding) => {  %>
                <div class="col-xs-6 col-sm-4 col-md-3">
                    <div class="album-list-item">
                      <a href="/wedding/<%= wedding.folder_name %>" class="album-link">
                        <img class="d-block img-fluid border-0" src="<%= wedding.image_link %>" alt="">
                        <i class="bi bi-play"></i>
                      </a>
                    </div>
                    <div class="album-caption pt-2 text-center">
                      <h6 class="text-uppercase"><%= wedding.album_name %></h6>
                    </div>
                  </div>
           <% }) %>
        </div>
    </div>
  </section>
<%- include('partials/footer') -%>
